    .wrapper
      main.container
        nav.sub_menu
          ul
            li
              a.on(href="#") 국책 과제
            li
              a(href="#") 산학 과제
            li
              a(href="#") 내부 과제

        section.content
          table.p_content
            thead
              tr
                th English Title
                th Korean Title
                th Date
            tbody
              each val in [1,2,3,4,5,6,7,8]
                tr
                  td= '영문 과제명'+ val
                  td= '한글 과제명'+ val
                  td [xxxx.x.x ~ xxxx.x.x]
                    a(href='#')
                      i.fas.fa-info-circle